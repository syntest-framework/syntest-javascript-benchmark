{
  "target-root-directory": "./benchmark/express",
  "include": [
    "./benchmark/express/lib/middleware/query.js"
    ],
  "crossover": "javascript-tree",
  "sampler": "javascript-random",
  "termination-triggers": [
  ],
  "preset": "random",
  "constant-pool-probability": 0.5,
  "type-pool-probability": 0.5,
  "statement-pool-probability": 0.9,
  "delta-mutation-probability": 0.8,
  "string-alphabet": "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ !@#$%^&*()-_=+[]{};:'\"|\\,.<>/?~§±`'",
  "modules": [
    "@syntest/javascript",
    "@syntest/plugin-core-metric-middleware-file-writer"
  ],
  "console-log-level": "info",
  "file-log-level": [
    "info",
    "warn",
    "error",
    "debug"
  ],
  "metric-middleware-pipeline": ["metric-middleware-file-writer"],
  "output-metrics": ["*"],
  "total-time": 30
}